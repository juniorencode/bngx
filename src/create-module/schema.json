{
  "$schema": "http://json-schema.org/schema",
  "$id": "SchematicCreateModule",
  "title": "Crear Módulo",
  "type": "object",
  "properties": {
    "nameSpanish": {
      "type": "string",
      "description": "Nombre de Módulo en Español",
      "x-prompt": "Nombre (español, plural) [Registro de Actividades]:"
    },
    "nameEnglish": {
      "type": "string",
      "description": "Nombre de Módulo",
      "x-prompt": "Nombre (inglés, singular) [activity log]:"
    }
  },
  "required": ["nameEnglish", "nameSpanish"]
}
